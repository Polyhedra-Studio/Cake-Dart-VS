{
	"name": "cake-dart-tester",
	"displayName": "Cake Tester for Dart",
	"description": "Test interface for VS Code for Cake Test for Dart",
	"version": "1.0.0",
	"publisher": "Polyhedra",
	"private": true,
	"license": "MPL-2.0",
	"repository": "http://butterfree.lan:30120/dart/cake-vs.git",
	"engines": {
		"vscode": "^1.68.1"
	},
	"categories": [
		"Testing"
	],
	"icon": "cake_logo.png",
	"activationEvents": [
		"workspaceContains:**/*.cake.dart",
		"workspaceContains:*.cake.dart"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint . --ext .ts,.tsx",
		"watch": "tsc -watch -p ./",
		"download-api": "vscode-dts dev && vscode-dts main",
		"postinstall": "npm run download-api"
	},
	"devDependencies": {
		"@types/node": "^16.11.7",
		"@typescript-eslint/eslint-plugin": "^5.30.0",
		"@typescript-eslint/parser": "^5.30.0",
		"eslint": "^8.13.0",
		"typescript": "^4.7.2",
		"vscode-dts": "^0.3.3"
	}
}
 